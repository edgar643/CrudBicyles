// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource BikeTrackProDB {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model User {
  id  Int @default(autoincrement()) @id
  user_name String  
  password String
  rol String
}
model Bicycle {
  id  Int @default(autoincrement()) @id
  brand String  
  type String
  color String
}

model Event {
  id  Int @default(autoincrement()) @id
  name String  
  date DateTime
  location String
  competition Boolean
  prize Float
}

model Route {
  id Int @default(autoincrement()) @id
  name String  
  distance String
  difficulty String
  path String
 }

model Rental {
  id Int @default(autoincrement()) @id
  brand String  
  type  String
  color String
  status String
 }



